<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ECommerceCustomerOrder</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ECommerceCustomerOrder.styles.css" asp-append-version="true" />
    <link href="~/css/Customer.css" rel="stylesheet" />

</head>
<body>
    <header>


        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">

                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li>
                            <img src=~/css/images12.png alt="" width="150px" height="70" style="margin-bottom:10px;">

                        </li>
                 
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/CustomerMvc/CustomerView">Customer</a>
                        </li>
                     
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/ProductMvc/ProductView">Product</a>
                        </li>
                       
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/OrderMvc/OrderView">Order</a>
                        </li>
                        
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/OrderDetailMvc/OrderDetailView">OrderDetail</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/OrderDetailMvc/Invoice">InvoiceDetail</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

                       
            

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    

  @*  <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - ECommerceCustomerOrder - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>*@




    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
<style>

</style>
</html>